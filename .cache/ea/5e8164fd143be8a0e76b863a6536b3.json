) => {\n                const tl = gsap.timeline({\n                  scrollTrigger: {\n                    trigger: text,\n                    end: \"+=60%\",\n                  },\n                });\n            \n                tl.set(text, {\n                  autoAlpha: 0,\n                  y: this.randomY,\n                }).fromTo(\n                  text,\n                  {\n                    autoAlpha: 0,\n                  },\n                  {\n                    autoAlpha: 1,\n                    duration: 1.8,\n                    y: 0,\n                    duration: this.randomDuration,\n                    ease: \"power2.out\",\n                  }\n                );\n            });\n        })\n        \n        this.preload.onprogress = event => {\n            this.preloadProgress.textContent = event.progress + '%'\n        }\n    }\n}"},"lineCount":null}},"error":null,"hash":"8172e96954844b5b90bf9a43ed161f83","cacheData":{"env":{}}}